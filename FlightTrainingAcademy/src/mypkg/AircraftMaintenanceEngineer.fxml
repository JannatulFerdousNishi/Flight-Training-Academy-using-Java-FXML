<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mypkg.AircraftMaintenanceEngineerController">
   <children>
      <AnchorPane id="AnchorPane" layoutX="30.0" layoutY="30.0" prefHeight="312.0" prefWidth="483.0">
         <children>
            <Label alignment="CENTER" layoutX="39.0" layoutY="14.0" prefHeight="64.0" prefWidth="417.0" text="Aircraft Maintenance Engineer" textFill="#1323b4" textOverrun="CENTER_ELLIPSIS">
               <font>
                  <Font name="System Bold Italic" size="27.0" />
               </font>
            </Label>
            <VBox alignment="CENTER" layoutX="97.0" layoutY="73.0" prefHeight="213.0" prefWidth="275.0" spacing="10.0">
               <children>
                  <Button fx:id="performanceReportButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#performanceReportButtonOnAction" text="Performance Report" />
                  <Button fx:id="statusOfAirCraftButton" mnemonicParsing="false" onAction="#statusOfAirCraftButtonOnAction" text="Status Of Aircraft" />
                  <Button fx:id="troubleshootButton" mnemonicParsing="false" onAction="#troubleshootButtonOnAction" text="Troubleshoot" />
                  <Button fx:id="orderPartsButton" contentDisplay="CENTER" mnemonicParsing="false" onAction="#orderPartsButtonOnAction" text="Order Parts" />
                  <Button fx:id="ideasButton" mnemonicParsing="false" onAction="#ideasButtonOnAction" text="Ideas" />
               </children>
               <effect>
                  <SepiaTone />
               </effect>
            </VBox>
         </children>
      </AnchorPane>
      <Button layoutX="41.0" layoutY="342.0" mnemonicParsing="false" onAction="#backtoHomeOnAction" text="Back to Home" />
   </children>
</AnchorPane>
