<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mypkg.FlyingHoursHistoryController">
   <children>
      <AnchorPane id="AnchorPane" layoutX="10.0" layoutY="10.0" prefHeight="400.0" prefWidth="600.0">
         <children>
            <Label layoutX="146.0" layoutY="27.0" text="Flying Hours History">
               <font>
                  <Font name="System Bold Italic" size="30.0" />
               </font>
            </Label>
            <VBox alignment="TOP_CENTER" layoutX="39.0" layoutY="108.0" prefHeight="203.0" prefWidth="213.0" spacing="10.0">
               <children>
                  <Button fx:id="atircraftHoursButton" mnemonicParsing="false" onAction="#aircraftHoursButtonOnClick" text="Aircraft Hours" />
                  <Label fx:id="aircraftHoursLabel" prefHeight="153.0" prefWidth="219.0" text=" " />
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" layoutX="347.0" layoutY="108.0" prefHeight="192.0" prefWidth="213.0" spacing="10.0">
               <children>
                  <Button fx:id="simulatorHoursButton" mnemonicParsing="false" onAction="#simulatorHoursButtonOnClick" text="Simulator Hours" />
                  <Label fx:id="simulatorHoursLabel" prefHeight="153.0" prefWidth="219.0" text=" " />
               </children>
            </VBox>
            <HBox layoutX="23.0" layoutY="349.0" prefHeight="25.0" prefWidth="553.0" spacing="320.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#goBackToMainSceneOnClick" text="Go Back To Your Main Scene" />
                  <Button fx:id="logOutButton" mnemonicParsing="false" onAction="#logOutButtonOnClick" text="Log Out" />
               </children>
            </HBox>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
